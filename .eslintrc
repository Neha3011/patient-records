{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "comma-dangle": 0,
    "max-len": 0,
    "no-case-declarations": 0,
    "no-else-return": 0,
    "consistent-return": 0,
    "default-case": 0,
    "array-callback-return": 0,
    "space-before-function-paren": [1, {"anonymous": "never", "named": "never"}],
    "no-multi-spaces": 1,
    "no-param-reassign": 0,
    "no-nested-ternary": 0,
    "no-mixed-operators": 0,
    "new-cap": 0,
    "quotes": [1, "single", "avoid-escape"],
    "quote-props": [2, "always"],
    "arrow-body-style": [1, "always"],
    "prefer-const": 2,
    "prefer-template": 0,
    "one-var": [2, {
      "uninitialized": "always", // Exactly one declaration for uninitialized variables per function (var) or block (let or const)
      "initialized": "never" // Exactly one declarator per initialized variable declaration per function (var) or block (let or const)
    }],
    "one-var-declaration-per-line": [2, "initializations"],
    "import/prefer-default-export": "off",
    "react/prefer-es6-class": 1,
    "react/prop-types": 0,
    "react/jsx-no-bind": 0,
    "react/jsx-boolean-value": [1, "always"],
    "react/no-did-mount-set-state": 0,
    "react/prefer-stateless-function": 0,
    "react/sort-comp": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/anchor-is-valid": 0
  },
  "env": {
    "browser": true
  }
}
